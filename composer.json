{
    "name": "itk-dev/get-organized-api-php-client",
    "description": "PHP client for GetOrganized API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ItkDev\\GetOrganizedApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mikkel Ricky",
            "email": "rimi@aarhus.dk"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/http-client": "^5.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2"
    },
    "scripts": {
        "coding-standards-check": "php-cs-fixer fix --allow-risky=yes --dry-run",
        "coding-standards-apply": "php-cs-fixer fix --allow-risky=yes"
    }
}
